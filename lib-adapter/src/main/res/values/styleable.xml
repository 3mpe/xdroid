<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProxyViewBinder">
        <attr name="viewId" format="reference"/>
        <attr name="viewType" format="enum">
            <enum name="TextView" value="1"/>
            <enum name="CompoundButton" value="2"/>
            <enum name="RatingBar" value="3"/>
        </attr>
        <attr name="extractorMethod" format="string|reference"/>
        <attr name="extractorClass" format="string|reference"/>
        <attr name="binderClass" format="string|reference"/>
    </declare-styleable>
</resources>
